Script started on 2025-07-17 01:28:50+00:00 [TERM="xterm-256color" TTY="/dev/pts/11" COLUMNS="240" LINES="43"]
    subprocess.Popen(['osascript', '-e', script])


def open_gnome(container):
    subprocess.Popen([
        'gnome-terminal', '--', 'bash', '-c',
        f"docker exec -it {container} bash; exec bash"
    ])


def open_inline(container):
    subprocess.run(['docker', 'exec', '-it', container, 'bash'])


def handle_sigint(signum, frame):
    print("\nâš ï¸ Use the menu to quit instead of Ctrl+C!")


def choose_method(available_methods):
    print("\nğŸ“‹ Choose how to open the container session:")
    for idx, m in enumerate(available_methods, 1):
        print(f"  {idx}. {m}")
    print("  q. Quit")
    while True:
        choice = input("ğŸ‘‰ Your choice [number or 'q']: ").strip().lower()
        if choice == 'q':
            sys.exit(0)
        elif choice.isdigit() and 1 <= int(choice) <= len(available_methods):
            return available_methods[int(choice) - 1]
        print("âš ï¸ Invalid choice. Try again.")


def connect(container, method, project):
    if method == 'tmux':
        enable_tmux_mouse()
        show_tmux_cheat_sheet()
        open_tmux_split(container)
    elif method == 'iterm':
        open_iterm(container)
    elif method == 'gnome-terminal':
        open_gnome(container)
    else:
        open_inline(container)


def kill_tmux_session(project):
    if 'TMUX' in os.environ:
        try:
            subprocess.run(['tmux', 'kill-session', '-t', project])
            print(f"ğŸ›‘ tmux session '{project}' terminated.")
        except Exception as e:
            print(f"âš ï¸ Failed to kill tmux session: {e}")


def show_tmux_cheat_sheet():
    cheat = []
    color_prefix = "\033[96m"
    color_key = "\033[92m"
    color_reset = "\033[0m"

    def line(text=""):
        cheat.append(text)

    line(f"\nğŸ¹ {color_prefix}Tmux Cheat Sheet{color_reset}")
    line("â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€")
    line(f"ğŸª„ {color_key}Prefix:{color_reset} Ctrl+b")
    line(f"ğŸ”· Navigate Panes: {color_key}Prefix + arrows{color_reset}")
    line(f"ğŸ”· Split Horizontally: {color_key}Prefix + \"{color_reset}")
    line(f"ğŸ”· Split Vertically: {color_key}Prefix + %{color_reset}")
    line(f"ğŸ”· Zoom Pane: {color_key}Prefix + z{color_reset}")
    line(f"ğŸ”· Cycle Layouts: {color_key}Prefix + Space{color_reset}")
    line(f"ğŸ”· Detach: {color_key}Prefix + d{color_reset}")
    line("â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€â”€\n")

    print("\n".join(cheat))
    plain = "\n".join(strip_ansi(c) for c in cheat)
    with open("tmux-cheatsheet.txt", "w") as f:
        f.write(plain)
    print("ğŸ“„ Cheat sheet saved to: tmux-cheatsheet.txt")


def strip_ansi(text):
    return re.sub(r'\x1B\[[0-?]*[ -/]*[@-~]', '', text)


def main_menu():
    while True:
        print("\nğŸ“‹ What would you like to do?")
        print("  1. ğŸš€ Start lab (docker-compose up -d)")
        print("  2. ğŸ”— Connect to containers")
        print("  3. ğŸ›‘ Stop lab (docker-compose down)")
        print("  q. âŒ Quit")
        choice = input("ğŸ‘‰ Your choice: ").strip().lower()

        if choice == '1':
            start_lab()
        elif choice == '2':
            connect_to_lab()
        elif choice == '3':
            stop_lab()
        elif choice == 'q':
            print("ğŸ‘‹ Goodbye!")
            sys.exit(0)
        else:
            print("âš ï¸ Invalid choice.")


def start_lab():
    if not os.path.exists('docker-compose.yml'):
        print("âŒ docker-compose.yml not found!")
        return
    print("ğŸš€ Starting lab...")
    subprocess.run(['docker-compose', 'up', '-d'])


def stop_lab():
    if not os.path.exists('docker-compose.yml'):
        print("âŒ docker-compose.yml not found!")
        return
    print("ğŸ›‘ Stopping lab...")
    subprocess.run(['docker-compose', 'down'])


